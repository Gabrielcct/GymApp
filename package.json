    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^3.4.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
